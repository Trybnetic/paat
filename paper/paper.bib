	year = {2019},
	note = {Publisher: Human Kinetics
Section: Journal for the Measurement of Physical Behaviour},
	keywords = {functional comparison paper, paat-paper, paper1, paper2},
	pages = {188--196},
	file = {Full Text PDF:/home/msw/Zotero/storage/P52K94IT/Migueles et al. - 2019 - GGIR A Research Community–Driven Open Source R Pa.pdf:application/pdf;Snapshot:/home/msw/Zotero/storage/IAKUQ7ZU/article-p188.html:text/html},
