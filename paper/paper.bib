@article{weitz_influence_2024,
	title = {Influence of {Accelerometer} {Calibration} on the {Estimation} of {Objectively} {Measured} {Physical} {Activity}: {The} {Tromsø} {Study}},
	volume = {7},
	issn = {2575-6605, 2575-6613},
	shorttitle = {Influence of {Accelerometer} {Calibration} on the {Estimation} of {Objectively} {Measured} {Physical} {Activity}},
	url = {https://journals.humankinetics.com/view/journals/jmpb/7/1/article-jmpb.2023-0019.xml},
	doi = {10.1123/jmpb.2023-0019},
	abstract = {Accelerometers are increasingly used to observe human behavior such as physical activity under free-living conditions. An important prerequisite to obtain reliable results is the correct calibration of the sensors. However, accurate calibration is often neglected, leading to potentially biased results. Here, we demonstrate and quantify the effect of accelerometer miscalibration on the estimation of objectively measured physical activity under free-living conditions. The total volume of moderate to vigorous physical activity (MVPA) was significantly reduced after post hoc auto-calibration for uniaxial and triaxial count data, as well as for Euclidean Norm Minus One and mean amplitude deviation raw data. Weekly estimates of MVPA were reduced on average by 5.5, 9.2, 45.8, and 4.8 min, respectively, when compared to the original uncalibrated estimates. Our results indicate a general trend of overestimating physical activity when using factory-calibrated sensors. In particular, the accuracy of estimates derived from the Euclidean Norm Minus One feature suffered from uncalibrated sensors. For all modalities, the more uncalibrated the sensor was, the more MVPA was overestimated. This might especially affect studies with lower sample sizes.},
	language = {en},
	number = {1},
	urldate = {2024-01-24},
	journal = {Journal for the Measurement of Physical Behaviour},
	author = {Weitz, Marc and Morseth, Bente and Hopstock, Laila A. and Horsch, Alexander},
	month = jan,
	year = {2024},
	note = {Publisher: Human Kinetics
Section: Journal for the Measurement of Physical Behaviour},
	keywords = {paat-paper, paper1, thesis},
	file = {Full Text PDF:/home/msw/Zotero/storage/2W6L7ZTJ/Weitz et al. - 2024 - Influence of Accelerometer Calibration on the Esti.pdf:application/pdf},
